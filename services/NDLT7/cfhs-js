PIDFILE="/tmp/run.neruthes/cfhs-js.pid/new"

function start() {
    cfhs-js-ctl s new
}
function stop() {
    # cfhs-js-ctl e new
    sudo start-stop-daemon --stop --pidfile "$PIDFILE" --user "$USER" --verbose
}
function restart() {
    stop
    start
}
function getpid() {
    sudo cat "$PIDFILE" 2>/dev/null
}
