function start() {
    start-stop-daemon --start --pidfile "$PIDFILE" --make-pidfile -b --exec /usr/local/bin/gost -- -L=ss://chacha20-ietf-poly1305:$(cat $HOME/.config/sskey)@:23080 -F=socks5://10.0.233.3:7891
}
function stop() {
    start-stop-daemon --stop --pidfile "$PIDFILE" --verbose
}
function restart() {
    stop
    start
}
